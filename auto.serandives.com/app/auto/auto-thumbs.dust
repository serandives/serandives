<div class="auto-listing" style="background-color: {color}">
    <h2>Page Index : {index}</h2>
    {#vehicles}
        <div class="vehicle">
            <div class="title">{title}</div>
            <div class="price">{price.amount} {price.currency}</div>
            <div class="make">By {make.brand} in {make.year}</div>
        </div>
        <hr/>
    {/vehicles}
</div>